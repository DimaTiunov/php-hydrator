{
  "name": "alexpts/php-hydrator",
  "description": "Convert model to dto, convert dto to model",
  "homepage": "https://github.com/alexpts/php-hydrator",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "model",
    "converter",
    "transform",
    "dto",
    "data",
    "mapper"
  ],
  "config": {
    "optimize-autoloader": true
  },
  "authors": [
    {
      "name": "Alexpts",
      "email": "alexpts3@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "fzaninotto/faker": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "suggest": {
    "ext-blackfire": "*",
    "blackfire/php-sdk": "^1.30"
  },
  "autoload": {
    "psr-4": {
      "PTS\\Hydrator\\": "src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --config=test/phpunit.xml"
  }
}
